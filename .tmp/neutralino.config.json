{
    "applicationId": "com.projectpulse.app",
    "version": "1.0.0",
    "defaultMode": "window",
    "port": 0,
    "documentRoot": "./",
    "url": "/",
    "enableServer": true,
    "enableNativeAPI": true,
    "tokenSecurity": "one-time",
    "logging": {
        "enabled": true,
        "writeToLogFile": true
    },
    "nativeAllowList": ["app.*", "os.*", "filesystem.*", "notification.*", "storage.*", "window.*"],
    "globalVariables": {
        "TEST1": "Hello",
        "TEST2": [2, 4, 5],
        "TEST3": {
            "value": 10
        }
    },
    "modes": {
        "window": {
            "title": "Project Pulse",
            "width": 1200,
            "height": 800,
            "minWidth": 800,
            "minHeight": 600,
            "fullScreen": false,
            "alwaysOnTop": false,
            "icon": "/resources/icons/appIcon.png",
            "enableInspector": false,
            "borderless": false,
            "maximize": false,
            "hidden": false,
            "resizable": true,
            "exitProcessOnClose": true
        }
    },
    "cli": {
        "binaryName": "project-pulse-ui",
        "resourcesPath": "/resources/",
        "extensionsPath": "/extensions/",
        "clientLibrary": "/resources/js/neutralino.js",
        "binaryVersion": "4.14.1",
        "clientVersion": "3.12.0"
    }
}
